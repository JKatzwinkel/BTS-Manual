 <topicref class="- map/topicref " href="oberflaeche_e1.dita" type="concept" xtrc="topicref:7;13:39" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">UI Reference </navtitle><?ditaot gentext?><linktext class="- map/linktext ">UI Reference</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">General description of the UI after the programm start. Definition of the major parts of     the UI. </shortdesc></topicmeta><topicref class="- map/topicref " href="oberflaeche_menuleiste.dita" type="topic" xtrc="topicref:8;14:48" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Menu bar</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Menu bar</linktext></topicmeta><topicref class="- map/topicref " href="oberflaeche_file.dita" type="concept" xtrc="topicref:9;15:44" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">File</navtitle><?ditaot gentext?><linktext class="- map/linktext ">File</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Clicking on &quot;File&quot; opens a menu with the entries &quot;New&quot; / &quot;New Project&quot;, &quot;Insert&quot;,   &quot;ExitBTS&quot; and &quot;RestartBTS&quot;. The following will provide the description of each entry.</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_edit.dita" type="concept" xtrc="topicref:10;16:44" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Edit</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Edit</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">The &quot;Edit&quot; menu contains the following enties: &quot;Edit&quot;, &quot;Edit current project&quot;, &quot;Edit   Updaters/Readers&quot;, &quot;Open Conflict Dialog&quot;, &quot;Open Revision History&quot;</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_analyse.dita" type="concept" xtrc="topicref:11;17:47" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Analyse</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Analyse</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">The  &quot;Analyse&quot; menu contains entry &quot;Analyze Text...&quot;</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_window.dita" type="concept" xtrc="topicref:12;18:46" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Window</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Window</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">The entries of the &quot;Window&quot; menu are the following:</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_preferences.dita" type="concept" xtrc="topicref:13;19:51" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Preferences</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Preferences</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Dropdown menu &quot;Preferences&quot; contains the following entries:</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_help.dita" type="concept" xtrc="topicref:14;20:44" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Help</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Help</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Options of the &quot;Help&quot; menu</shortdesc></topicmeta></topicref></topicref><topicref class="- map/topicref " href="oberflaeche_toolbar_e3.dita" type="topic" xtrc="topicref:15;22:49" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Tool bar </navtitle><?ditaot gentext?><linktext class="- map/linktext ">Tool bar</linktext></topicmeta></topicref><topicref class="- map/topicref " href="statusleiste_e8.dita" type="topic" xtrc="topicref:16;23:42" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Status bar </navtitle><?ditaot gentext?><linktext class="- map/linktext ">Status bar</linktext></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_workspace.dita" type="concept" xtrc="topicref:17;24:47" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Work space</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Work space</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">&quot;Work space&quot; the part of the BTS user interface, located between tool bar and status                 bar. It changes appearance when the user switchs between &quot;Corpus&quot;, &quot;Lemma&quot; or                 &quot;Thesaurus&quot; views. </shortdesc></topicmeta><topicref class="- map/topicref " href="oberflaeche_funktionen_e5.dita" type="topic" xtrc="topicref:18;25:53" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Common features </navtitle><?ditaot gentext?><linktext class="- map/linktext ">Common features</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">BTS-workspace consists of dynamic windows, which can be hidden, moved and resized. Each     window can contain one or more tabs. Tabs can be moved from one window to another or opened in a     separate window. </shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_corpora.dita" type="concept" xtrc="topicref:19;26:46" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Corpora</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Corpora</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">The &quot;Corpora&quot; is the default view of the BTS&apos;s work space. Here, you can work on   objects, texts, translations and hieroglyphs.</shortdesc></topicmeta><topicref class="- map/topicref " href="oberflaeche_corpusnavigator_e9.dita" type="concept" xtrc="topicref:20;27:59" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Corpus Navigator </navtitle><?ditaot gentext?><linktext class="- map/linktext ">Corpus Navigator</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">The &quot;Corpus Navigator&quot; tab is by default located on the left side of the &quot;Corpus&quot; work   space. Here you can see the position of the text in the corpus, which is displayed hierarchically.   You can navigate through the hierarchical &quot;tree&quot; by clicking or using the &quot;up&quot; and &quot;down&quot; arrow   keys on your keyboard.</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_texteditor_e10.dita" type="concept" xtrc="topicref:21;28:55" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Texteditor</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Texteditor</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Overview over the functions of the Texteditor</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_passporteditor.dita" type="concept" xtrc="topicref:22;29:55" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Passport Editor</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Passport Editor</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">A short description of the functions found in the &quot;Passport Editor&quot; window.</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_lemmatizer.dita" type="concept" xtrc="topicref:23;30:51" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Lemmatizer</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Lemmatizer</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">The Lemmatizer is used to reference the selected transliterated word to the lemma-entry   in the lemma list. It has a toolbar and an input-surface.</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="corpus_htw.dita" type="concept" xtrc="topicref:24;31:39" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Hieroglyph Type Writer (HTW)</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Hieroglyph Type Writer (HTW)</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">&quot;Hieroglyph Type Writer&quot; is a tool for entering or editing hieroglyphs for the lemmas   displayed in the &quot;Sign-Text-Editor&quot; tab. &quot;Hieroglyph Type Writer&quot; can be found in the lower part   of the BTS UI in the default view, next to the &quot;Passport Editor&quot; and &quot;Lemmatizer&quot;.</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_annotationsfenster.dita" type="concept" xtrc="topicref:25;32:59" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Annotation tab</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Annotation tab</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">In the default view of the BTS user interface, the annotations window is placed on the   right. It shows all annotations of the active text.</shortdesc></topicmeta></topicref></topicref><topicref class="- map/topicref " href="oberflaeche_lemmanaview.dita" type="concept" xtrc="topicref:26;34:50" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Lemma</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Lemma</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc "></shortdesc></topicmeta><topicref class="- map/topicref " href="oberflaeche_lemmanavigator.dita" type="concept" xtrc="topicref:27;35:55" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Lemma Navigator</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Lemma Navigator</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc "></shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_lemma_lemmaeditor.dita" type="concept" xtrc="topicref:28;36:58" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Lemma Editor</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Lemma Editor</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Lemma Editor Tab. The Lemma Editor is located in the upper centre of the User Interface   while Lemma View is active.</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_lemma_passporteditor.dita" type="concept" xtrc="topicref:29;37:61" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Passport Editor (Lemma)</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Passport Editor (Lemma)</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">The Passport Editor for lemmata is located in the lower centre of the User     Interface.</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_lemmatizer.dita" type="concept" xtrc="topicref:30;38:51" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Lemmatizer</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Lemmatizer</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">The Lemmatizer is used to reference the selected transliterated word to the lemma-entry   in the lemma list. It has a toolbar and an input-surface.</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_lemma_annotation.dita" type="concept" xtrc="topicref:31;39:57" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Annotation Part (Lemma)</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Annotation Part (Lemma)</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Annotation Part in Lemma view. The annotation window is on the right of the User   Interface. <?oxy_custom_start type="oxy_content_highlight" color="255,255,0"?>In Lemma view it is   currently without function.<?oxy_custom_end ?></shortdesc></topicmeta></topicref></topicref><topicref class="- map/topicref " href="oberflaeche_thesaurus.dita" type="concept" xtrc="topicref:32;41:48" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Thesaurus</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Thesaurus</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Thesaurus view</shortdesc></topicmeta><topicref class="- map/topicref " href="oberflaeche_thesaurusnavigator.dita" type="concept" xtrc="topicref:33;42:59" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Thesaurus Navigator</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Thesaurus Navigator</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc "></shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_thesaurs_passporteditor.dita" type="concept" xtrc="topicref:34;43:64" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Passport Editor (genauere Erklärung nötig)</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Passport Editor (genauere Erklärung nötig)</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Functions of Passport Editor in Thesaurus view. The Passport Editor is located in the   centre of the User Interface in the Thesaurus view. Here, you can enter metadata about your   Thesaurus entries, e.g. dating and coordinates, or links to external websites   -<?oxy_custom_start type="oxy_content_highlight" color="255,255,0"?>-&gt; genauer   erklären<?oxy_custom_end ?>.</shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="oberflaeche_thesaurus_annotation.dita" type="concept" xtrc="topicref:35;44:61" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Annotation part (Thesaurus)</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Annotation part (Thesaurus)</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Functions of the annotation part in Thesaurus view</shortdesc></topicmeta></topicref></topicref></topicref></topicref>
 <topicref class="- map/topicref " href="howto.dita" type="concept" xtrc="topicref:36;48:30" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">&quot;How to&quot;-Topics</navtitle><?ditaot gentext?><linktext class="- map/linktext ">&quot;How to&quot;-Topics</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">Here you get short introductions that allow you to start working fastly. For more   in-depth information consult the User Interface section. </shortdesc></topicmeta><topicref class="- map/topicref " href="corpus.dita" type="concept" xtrc="topicref:37;49:32" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Corpus</navtitle><?ditaot gentext?><linktext class="- map/linktext ">Corpus</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc "></shortdesc></topicmeta><topicref class="- map/topicref " href="howto_createcorpus.dita" type="task" xtrc="topicref:38;50:46" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">How to create a corpus</navtitle><?ditaot gentext?><linktext class="- map/linktext ">How to create a corpus</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc "></shortdesc></topicmeta></topicref><topicref class="- map/topicref " href="corpus_texteingabe.dita" type="concept" xtrc="topicref:39;51:45" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">How to create a text</navtitle><?ditaot gentext?><linktext class="- map/linktext ">How to create a text</linktext><?ditaot genshortdesc?><shortdesc class="- map/shortdesc "></shortdesc></topicmeta><topicref class="- map/topicref " href="transliteration.dita" type="concept" xtrc="topicref:40;52:44" xtrf="P:\Handbuch\BTS.ditamap"><topicmeta class="- map/topicmeta "><navtitle class="- topic/navtitle ">Transliteration</navtitle><?ditaot gentext?><linktext class="- m