       <entry colname="col2" class="- topic/entry " xtrf="C:\Users\boehme\Documents\GitHub\BTS-Manual\grammar_destructions.dita" xtrc="entry:4;54:15"> “--3Q--“</entry>
      </row>
      <row class="- topic/row " xtrf="C:\Users\boehme\Documents\GitHub\BTS-Manual\grammar_destructions.dita" xtrc="row:3;56:12">
