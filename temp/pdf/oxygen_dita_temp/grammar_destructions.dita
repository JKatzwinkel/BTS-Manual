       <entry colname="col2" class="- topic/entry " xtrf="C:\Users\boehme\Documents\GitHub\BTS-Manual\grammar_destructions.dita" xtrc="entry:2;50:15"> </entry>
      </row>
      <row class="- topic/row " xtrf="C:\Users\boehme\Documents\GitHub\BTS-Manual\grammar_destructions.dita" xtrc="row:2;52:12">
